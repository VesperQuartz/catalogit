{
  "type": "module",
  "name": "api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "generate": "npx drizzle-kit generate",
    "migrate": "npx drizzle-kit migrate",
    "studio": "npx drizzle-kit studio",
    "push": "npx drizzle-kit push",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/csrf-protection": "^7.0.1",
    "@fastify/env": "^5.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/one-line-logger": "^2.0.0",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/redis": "^7.0.1",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@fastify/websocket": "^11.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@xenova/transformers": "^2.17.2",
    "aws-sdk": "^2.1691.0",
    "chromadb": "^1.9.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-orm": "^0.35.2",
    "drizzle-zod": "^0.5.1",
    "fastify": "^5.0.0",
    "fastify-bcrypt": "^1.0.1",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "paseto": "^3.1.4",
    "pg": "^8.13.0",
    "remeda": "^2.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.10",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "drizzle-kit": "^0.26.2",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.4.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
